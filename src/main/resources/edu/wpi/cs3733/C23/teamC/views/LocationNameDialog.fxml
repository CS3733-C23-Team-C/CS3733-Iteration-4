<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane expanded="true" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox spacing="10.0">
         <children>
            <Label text="Long Name" />
            <MFXTextField fx:id="longName" />
            <Label text="Short Name" />
            <MFXTextField fx:id="shortName" />
            <Label text="Location Type" />
            <MFXTextField fx:id="locationType" />
         </children>
      </VBox>
   </content>
   <buttonTypes>
      <ButtonType fx:id="okButton" buttonData="OK_DONE" text="OK" />
      <ButtonType fx:id="cancelButton" buttonData="CANCEL_CLOSE" text="Cancel" />
   </buttonTypes>
</DialogPane>
