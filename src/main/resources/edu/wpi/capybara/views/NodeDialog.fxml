<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="400.0" prefWidth="6.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox spacing="10.0">
         <children>
            <Label text="Node ID" />
            <MFXTextField fx:id="nodeID">
               <VBox.margin>
                  <Insets />
               </VBox.margin></MFXTextField>
            <Label text="X Coord" />
            <MFXTextField fx:id="xCoord" />
            <Label text="Y Coord" />
            <MFXTextField fx:id="yCoord" />
            <Label text="Floor" />
            <MFXTextField fx:id="floor" />
            <Label text="Building" />
            <MFXTextField fx:id="building" />
         </children>
      </VBox>
   </content>

   <buttonTypes>
      <ButtonType fx:id="okButton" buttonData="OK_DONE" text="OK" />
      <ButtonType buttonData="CANCEL_CLOSE" text="Cancel" />
   </buttonTypes>
</DialogPane>
